class Solution{


    static int searchInSorted(int arr[], int N, int K) {

        // Your code here
        for(int i=0;i<N;i++){
            if(K==arr[i]){
                return i+1;
            }
        }
        return -1;
    }


  public static void main (String args[]){
   int arr[] = {1,2,3,4,6};
   int k =6;
  System.out.println(searchInSorted(arr,5,k));
}

}
